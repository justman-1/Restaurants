<!DOCTYPE html>
<html>
<head>
	<link rel="stylesheet" type="text/css" href="/main.css">
	<title>Main</title>
</head>
<body>

	<form class='signupForm' action='/signup' method='post'>
		<div class="signupBack">Back</div>
		<div class='names'>
			<input type="text" name="firstName" placeholder="First name" class='namesPart' id='namesPart1'>
		    <input type="text" name="lastName" placeholder="Last name" class='namesPart' id='namesPart2'>
		</div>
		<input type="email" name="email" placeholder="your_email@gmail.com" class='formInp' id='formInp1'>
		<input type="password" name="password" placeholder="password" class='formInp' id='formInp2'>

		<input type="button" value='Sign up' class='signupSubmit'>
		<div class='signupError'></div>
		<div class="loading3">
			<img src="/imgs/reset.svg" class='loadingIm'>
		</div>
	</form>

	<form class='loginForm' action='/login' method='post'>
		<div class="loginBack">Back</div>
		<input type="email" name="email" placeholder="your_email@gmail.com" class='formInp' id='formInp3'>
		<input type="password" name="password" placeholder="password" class='formInp' id='formInp4'>
		<input type="button" value='Log in' class='loginSubmit'>
		<div class='loginError'></div>
		<div class="loading4">
			<img src="/imgs/reset.svg" class='loadingIm'>
		</div>
	</form>

	<header>
		<div class='headerEmpty'></div>
		<input type="text" class='searchInp' placeholder="Seacrh restaurant...">
		<div class="searchDiv">
			<img src="/search.svg" class='searchBut'>
		</div>
		<div class='log'>
			<div class='signUpBut'>Sign up</div>
			<div class='logInBut'>Log in</div>
		</div>
		<div class="myImage">
			<a href='feed'><img class="myImageImg" src='/user.png'></a>
		</div>
	</header>
	<section class='mainSect'>

		<nav>
			<div class='countriesName'>Countries</div>
			<div class='allCuisines'>
				<div class='cuisine'>
					<input type="checkbox" class='asianCheck'>
					<div class='asian'>Asian</div>
				</div>
				<div class='cuisine'>
					<input type="checkbox" class='italianCheck'>
					<div class='italian'>Italian</div>
				</div>
				<div class='cuisine'>
					<input type="checkbox" class='europeanCheck'>
					<div class='european'>European</div>
				</div>
				<div class='cuisine'>
					<input type="checkbox" class='chineseCheck'>
					<div class='chinese'>Ð¡hinese</div>
				</div>
				<div class='cuisine'>
					<input type="checkbox" class='russianCheck'>
					<div class='russian'>Russian</div>
				</div>
				<div class='cuisine'>
					<input type="checkbox" class='belarusianCheck'>
					<div class='belarusian'>Belarusian</div>
				</div>
				<div class='cuisine'>
					<input type="checkbox" class='japaneseCheck'>
					<div class='japanese'>Japanese</div>
				</div>
				<div class='cuisine'>
					<input type="checkbox" class='frenchCheck'>
					<div class='french'>French</div>
				</div>
				<div class='cuisine'>
					<input type="checkbox" class='turkishCheck'>
					<div class='turkish'>Turkish</div>
				</div>
			</div>
			<div class='additionalName'>Additional</div>
			<div class='additionals'>
				<div class="additional">
					<input type="checkbox" class='deliveryCheck'>
					<div class='delivery'>Delivery</div>
				</div>
				<div class="additional">
					<input type="checkbox" class='takeoutCheck'>
					<div class='takeout'>Takeout</div>
				</div>
			</div>
		</nav>

		<section class='allRestaurants'>
			<div class="loading">
				<img src="/imgs/reset.svg" class='loadingIm'>
			</div>
			<div class='restaurants'>
				
			</div>
		</section>

		<div class='recommended'>
			<div class='recommendedName'>Recommended</div>
			<div class="loading2">
				<img src="/imgs/reset.svg" class='loadingIm2'>
			</div>
			<div class="allRecommends">
				
			</div>
		</div>
	</section>
<script src="https://ajax.googleapis.com/ajax/libs/jquery/3.1.1/jquery.min.js"></script>
<script src='/main.js'></script>
<script src='forms.js'></script></body>
</html>